{
  "manifest_version": 3,
  "name": "BV SHOP 出貨助手",
  "version": "1.5.0",
  "description": "自動抓取嘉里大榮物流單",
  
  "permissions": ["storage"],
  
  "host_permissions": [
    "https://bvshop-manage.bvshop.tw/*"
  ],
  
  "content_scripts": [
    {
      "matches": ["https://bvshop-manage.bvshop.tw/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
